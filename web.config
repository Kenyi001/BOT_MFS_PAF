<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <appSettings>
        <add key="WSGI_HANDLER" value="app.app" />
        <add key="PYTHONPATH" value="C:\path\to\BOT_MFS_PAF" />
    </appSettings>
    <system.webServer>
        <handlers>
            <add name="FlaskApp"
                 path="*"
                 verb="*"
                 modules="FastCgiModule"
                 scriptProcessor="C:\path\to\BOT_MFS_PAF\venv\Scripts\python.exe|C:\path\to\BOT_MFS_PAF\venv\Lib\site-packages\wfastcgi.py"
                 resourceType="Unspecified"
                 requireAccess="Script" />
        </handlers>
    </system.webServer>
</configuration>
